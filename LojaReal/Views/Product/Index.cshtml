@model IEnumerable<DataAccess.Models.Product>

@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Category</th>
    </tr>
    @foreach (var c in Model)
    {
        <tr>
            <td>@c.ProductId</td>
            <td>@c.Name</td>
            <td>@c.Category.Name</td>
        </tr>
    }
</table>